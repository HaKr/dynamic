{
  "name": "dynamic",
  "version": "1.0.0",
  "description": "Dynamic templates that appear on value changes",
  "main": "dynamic-server.js",
  "scripts": {
    "testing": "browserify test/*.js | tape-run",
    "test": "tape -r ./src/modules/dynamic-values.js test/*.js",
    "abc": "tape-run",
    "watch": "npm-watch",
    "taper": "tape-run",
    "browserify": "browserify src/modules/*.js --outfile js/dynamic-app.js",
    "copy-shared": "cp -r dynamic-app.* js /media/sf_eeg/dynamic/"
  },
  "watch": {
    "browserify": "src/modules/*.js",
    "copy-shared": [  "js/*.js",  "dynamic-app.*" ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/HaKr/dynamic.git"
  },
  "keywords": [
    "Dynamic",
    "Web"
  ],
  "author": "Harry de Kroon",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.1.1",
    "event-emitter": "^0.3.4",
    "install": "^0.8.2",
    "npm": "^4.0.5",
    "npm-watch": "^0.1.6",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5",
    "tapes": "^4.1.0"
  }
}
